generator client {
  provider = "prisma-client"
	output = "./generated"
	engineType = "client"
	runtime = "bun"
}

datasource db {
  provider = "postgresql"
}

enum Status {
  UNFOLLOWED
  NOT_EXISTED
  APPROVED
}

model Profile {
  username    String   @id
  url         String  
  timestamp   BigInt
  status      Status?
}